            xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="UI.Client.RegistrationController">
    <children>
        <TextField fx:id="firstName" layoutX="131.0" layoutY="198.0" prefHeight="29.0" prefWidth="89.0"
                   promptText="First Name"
                   style="-fx-background-color: #204060#204060; -fx-border-color: #19334d#19334d; -fx-prompt-text-fill: #9fbfdf#9fbfdf; -fx-text-fill: #9fbfdf#9fbfdf #9fbfdf#9fbfdf #9fbfdf#9fbfdf #9fbfdf#9fbfdf;"/>
        <TextField fx:id="lastName" layoutX="225.0" layoutY="198.0" onAction="#clickedLastNam" prefHeight="29.0"
                   prefWidth="86.0" promptText="Last Name"
                   style="-fx-background-color: #204060#204060; -fx-border-color: #19334d#19334d; -fx-prompt-text-fill: #9fbfdf#9fbfdf; -fx-text-fill: #9fbfdf#9fbfdf #9fbfdf#9fbfdf #9fbfdf#9fbfdf #9fbfdf#9fbfdf;"/>
        <TextField fx:id="username" layoutX="132.0" layoutY="232.0" onAction="#clickedUsernam" prefHeight="29.0"
                   prefWidth="178.0" promptText="UserName"
                   style="-fx-background-color: #204060#204060; -fx-border-color: #19334d#19334d; -fx-prompt-text-fill: #9fbfdf#9fbfdf; -fx-text-fill: #9fbfdf#9fbfdf #9fbfdf#9fbfdf #9fbfdf#9fbfdf #9fbfdf#9fbfdf;"/>
        <PasswordField fx:id="password" accessibleRole="PASSWORD_FIELD" layoutX="132.0" layoutY="263.0"
                       onAction="#clickedPassword" prefHeight="29.0" prefWidth="178.0" promptText="Password"
                       style="-fx-background-color: #204060#204060; -fx-border-color: #19334d#19334d; -fx-prompt-text-fill: #9fbfdf#9fbfdf; -fx-text-fill: #9fbfdf#9fbfdf #9fbfdf#9fbfdf #9fbfdf#9fbfdf #9fbfdf#9fbfdf;"/>
        <PasswordField fx:id="confirmPassword" accessibleRole="PASSWORD_FIELD" layoutX="132.0" layoutY="295.0"
                       onAction="#clickedConfirmPassword" prefHeight="29.0" prefWidth="178.0"
                       promptText="Confirm Password"
                       style="-fx-background-color: #204060#204060; -fx-border-color: #19334d#19334d; -fx-prompt-text-fill: #9fbfdf#9fbfdf; -fx-text-fill: #9fbfdf#9fbfdf #9fbfdf#9fbfdf;">
